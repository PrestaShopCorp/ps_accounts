{
  "name": "module_aos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --no-clean",
    "watch": "vue-cli-service build --mode development --watch --no-clean dotenv_config_path=./.env",
    "lint": "vue-cli-service lint",
    "prettier-check": "prettier --check './ui/**/*.+(js|json|vue|scss)' './views/**/*.+(js|json|vue|scss)'",
    "prettier-write": "prettier --write './ui/**/*.+(js|json|vue|scss)' './views/**/*.+(js|json|vue|scss)'"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.1.0",
    "child_process": "^1.0.2",
    "core-js": "^3.4.3",
    "lodash": "^4.17.15",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "prettier": "^1.19.1",
    "vue-template-compiler": "^2.6.10"
  },
  "watch": {
    "test": {
      "patterns": [
        "ui"
      ],
      "extensions": "js,jsx",
      "quiet": false,
      "legacyWatch": true,
      "delay": 2500,
      "runOnChangeOnly": false
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
