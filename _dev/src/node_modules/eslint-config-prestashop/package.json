{
  "name": "eslint-config-prestashop",
  "version": "0.0.2",
  "description": "PrestaShop's JS ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --report-unused-disable-directives .",
    "test": "npm run --silent tests-only",
    "tests-only": "babel-tape-runner ./tests/test-*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PrestaShop/eslint-config-prestashop.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "styleguide"
  ],
  "author": "PrestaShop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PrestaShop/eslint-config-prestashop/issues"
  },
  "homepage": "https://github.com/PrestaShop/eslint-config-prestashop#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-preset-airbnb": "^3.2.0",
    "babel-tape-runner": "^3.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "tape": "^4.9.2"
  },
  "peerDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
