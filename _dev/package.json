{
  "name": "ps_accounts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "vue-cli-service build --no-clean --mode development --watch",
    "test": "vue-cli-service test:unit tests/*",
    "build": "vue-cli-service build --no-clean",
    "cs:check": "prettier --check './**/*.+(js|json|vue|scss)'",
    "cs:format": "prettier --write './**/*.+(js|json|vue|scss)'"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "rootDir": "./",
    "testRegex": ".spec.js$",
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests",
      "<rootDir>/src"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap-vue": "^2.1.0",
    "core-js": "^3.4.3",
    "eslint-plugin-vue-libs": "^4.0.0",
    "prestakit": "^1.1.0",
    "vue": "^2.6.10",
    "vue-i18n": "^8.15.3",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2",
    "@babel/preset-env": "^7.10.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "^4.2.2",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^25.1.0",
    "jsdom-global": "^3.0.2",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.10"
  }
}
